/*
 * Update product images on select change
 */
jQuery(document).ready(function($){

  var $thumbs = $(".js-slick-thumbs");
  var $featured = $(".js-slick-featured");
  
  // When one of the three options changes
  $('#ProductSelect-option-0').change(function() {
    changeImages();
  });
  $('#ProductSelect-option-1').change(function() {
    changeImages();
  });
  $('#ProductSelect-option-2').change(function() {
    changeImages();
  });
  
  function changeImages() {
    
    // Build array of current selections
    var selectedVariant = [];
    if ($('#ProductSelect-option-0').size()) { selectedVariant.push($('#ProductSelect-option-0').val()) }
    if ($('#ProductSelect-option-1').size()) { selectedVariant.push($('#ProductSelect-option-1').val()) }
    if ($('#ProductSelect-option-2').size()) { selectedVariant.push($('#ProductSelect-option-2').val()) }
    console.log('You selected: ', selectedVariant);
    
    // Remove current slides
    var imax = $thumbs.find('.slick-slide').size();
    for (i = imax; i >= 0; i--) { 
      console.log(i);
      $thumbs.slick('slickRemove', i)
      $featured.slick('slickRemove', i)
    }
    
    // Add new slides
    $.each(window.productImages, function( index, value ) {
      if (arraysEqual(value.alt, selectedVariant)) {
        console.log('found a match');
        $thumbs.slick('slickAdd','<div><img src="' + value.url.medium + '" alt="' + value.alt + '" /></div>');
        $featured.slick('slickAdd', '<div><a href="' + value.url.grande + '" class="js-mfp-product"><img src="' + value.url.grande + '" /></a></div>');
      }
    });
    
    theme.init();

  }
  
  // Utility function
  function arraysEqual(a, b) {
    if (a === b) return true;
    if (a == null || b == null) return false;
    if (a.length != b.length) return false;
  
    // If you don't care about the order of the elements inside
    // the array, you should sort both arrays here.
  
    for (var i = 0; i < a.length; ++i) {
      if (a[i] !== b[i]) return false;
    }
    return true;
  }
});